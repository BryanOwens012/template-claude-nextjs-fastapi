# Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# API Configuration
# Note: NEXT_PUBLIC_ prefix makes these variables available in the browser
NEXT_PUBLIC_API_URL=http://localhost:8000

# Add your frontend-specific environment variables below
# Examples:

# Authentication (if using NextAuth.js)
# NEXTAUTH_URL=http://localhost:3000
# NEXTAUTH_SECRET=generate_a_random_secret_here

# Analytics
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Feature Flags
# NEXT_PUBLIC_ENABLE_FEATURE_X=true

# External APIs (public keys only - never put secrets here with NEXT_PUBLIC_ prefix)
# NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxx
# NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=xxxxx
