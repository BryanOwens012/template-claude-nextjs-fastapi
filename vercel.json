{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/frontend && npm run build",
  "outputDirectory": "apps/frontend/.next",
  "devCommand": "cd apps/frontend && npm run dev",
  "installCommand": "cd apps/frontend && npm install",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  },
  "ignoreCommand": "bash -c 'if git diff --quiet HEAD^ HEAD -- apps/frontend apps/api vercel.json .env.example; then exit 1; else exit 0; fi'"
}
